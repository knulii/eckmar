{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-gmp": "*",
        "ext-xmlrpc": "*",
        "barryvdh/laravel-ide-helper": "^2.4",
        "defuse/php-encryption": "^2.4.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "fakerphp/faker" : "^1.20",
        "graham-campbell/markdown": "^14.0",
        "gregwar/captcha": "^1.2.1",
        "laravel/framework": "^9.0.0",
        "laravel/tinker": "^2.8.0",
        "nwidart/laravel-modules": "^5.0",
        "paragonie/sodium_compat": "^1.20",
        "phpseclib/phpseclib": "^3.0.33",
        "phpxmlrpc/phpxmlrpc": "^4.9.2",
        "predis/predis": "^2.2.2",
        "singpolyma/openpgp-php": "^0.6.0",
        "symfony/intl": "^6.3.0",
        "tamayo/laravel-scout-elastic": "^8.1.0",
        "webpatser/laravel-uuid": "^4.0",
        "elasticsearch/elasticsearch": "^7.12",
        "laravel/legacy-factories": "^1.3.2",
        "laravel/helpers": "^1.6.0",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "filp/whoops": "^2.15.0",
        "mockery/mockery": "^1.6.1",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postUpdate",
          "php artisan ide-helper:generate",
          "php artisan ide-helper:meta"
        ]

    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}